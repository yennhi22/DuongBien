{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "worker:dev": "nest start worker --watch",
    "worker:prod": "node --max-old-space-size=4096 dist/jobs/worker",
    "ga4:dev": "nest start ga4 --watch",
    "ga4:prod": "node dist/jobs/ga4",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "echo 'No e2e tests implemented yet.'",
    "migrate:create": "migrate create --template-file ./src/migrations/template.ts --migrations-dir=\"./src/migrations\" --compiler=\"ts:./src/migrations/utils/ts-compiler.js\"",
    "migrate:up": "migrate --migrations-dir=\"./src/migrations\" --compiler=\"ts:./src/migrations/utils/ts-compiler.js\" up",
    "migrate:down": "migrate --migrations-dir=\"./src/migrations\" --compiler=\"ts:./src/migrations/utils/ts-compiler.js\" down"
  },
  "dependencies": {
    "@analytics/google-analytics": "^1.0.5",
    "@escape.tech/graphql-armor": "^1.7.1",
    "@escape.tech/graphql-armor-block-field-suggestions": "^1.4.0",
    "@google-analytics/data": "^3.2.1",
    "@nestjs-modules/ioredis": "^1.0.1",
    "@nestjs/apollo": "^10.1.6",
    "@nestjs/axios": "^1.0.0",
    "@nestjs/common": "9.0.2",
    "@nestjs/core": "9.0.2",
    "@nestjs/graphql": "^10.1.7",
    "@nestjs/mongoose": "9.2.0",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "9.0.2",
    "@nestjs/throttler": "^3.1.0",
    "@types/passport": "^1.0.12",
    "@types/passport-http": "^0.3.9",
    "@types/passport-jwt": "^3.0.7",
    "analytics": "^0.8.1",
    "apollo-server-errors": "^3.3.1",
    "apollo-server-express": "^3.11.1",
    "bull": "^4.10.4",
    "cache-manager": "^4.0.0",
    "cache-manager-redis-store": "2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dataloader": "^2.1.0",
    "device": "^0.3.12",
    "dotenv": "^16.0.3",
    "express-device": "^0.4.2",
    "graphql": "^16.6.0",
    "graphql-query-complexity": "^0.12.0",
    "graphql-type-json": "^0.3.2",
    "ioredis": "^5.2.4",
    "ip": "^1.1.8",
    "jsonapi-serializer": "^3.6.7",
    "kafka-node": "^5.0.0",
    "kafkajs": "^2.2.3",
    "metadata-scraper": "^0.2.61",
    "migrate": "^1.8.0",
    "moment": "^2.29.4",
    "mongoose": "6.4.6",
    "mongoose-paginate-v2": "^1.7.1",
    "node-telegram-bot-api": "^0.61.0",
    "passport": "^0.6.0",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.5",
    "slugify": "^1.6.5",
    "typeorm": "^0.3.11",
    "ua-parser-js": "^1.0.35",
    "useragent": "^2.3.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@elastic/elasticsearch": "^8.5.0",
    "@nestjs/bull": "^0.6.3",
    "@nestjs/cli": "9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/elasticsearch": "^9.0.0",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/schematics": "9.0.1",
    "@nestjs/testing": "9.0.1",
    "@types/bull": "^4.10.0",
    "@types/cron": "^2.0.0",
    "@types/express": "4.17.13",
    "@types/jest": "28.1.4",
    "@types/jsonapi-serializer": "^3.6.5",
    "@types/node": "18.0.3",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "bodybuilder": "^2.5.0",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "jest": "28.1.2",
    "prettier": "2.7.1",
    "supertest": "6.2.4",
    "ts-jest": "28.0.5",
    "ts-loader": "9.3.1",
    "ts-node": "10.8.2",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
